{
  "name": "spore-documentation-theme",
  "version": "0.1.0",
  "description": "A documentation-theme for spore-kit",
  "main": "./dist/index.js",
  "scripts": {
    "dev": "gulp clean-test && documentation build ./test/src/index.js -f html -o ./test/dist -c ./documentation.yml -t ./src/index.js",
    "dist": "gulp build && gulp clean-test && documentation build ./test/src/index.js -f html -o ./test/dist -c ./documentation.yml -t ./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SporeUI/spore-documentation-theme.git"
  },
  "keywords": [
    "documentation",
    "theme"
  ],
  "author": "pillar0514@163.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SporeUI/spore-documentation-theme/issues"
  },
  "files": [
    "dist",
    "util",
    "README.md",
    "LICENSE"
  ],
  "homepage": "https://github.com/SporeUI/spore-documentation-theme#readme",
  "devDependencies": {
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-replace": "^0.6.1"
  },
  "dependencies": {
    "concat-stream": "^1.6.0",
    "doctrine-temporary-fork": "^2.0.0-alpha-allowarrayindex",
    "github-slugger": "^1.1.3",
    "globals-docs": "^2.3.0",
    "highlight.js": "^9.12.0",
    "lodash": "^4.17.4",
    "remark": "^8.0.0",
    "remark-html": "^6.0.1",
    "unist-builder": "^1.0.2",
    "unist-util-visit": "^1.1.3",
    "vinyl": "^2.1.0",
    "vinyl-fs": "^2.4.4"
  }
}
